-- 従業員モデルの入力データファクトリー定義
module UseCases.Services.Employee.InputEmployeeClass where
    import Presenters.Converters.Employee.InputEmployeeData

    class InputEmployeeClass a where
        getNameRaw :: a -> b
        getAgeRaw  :: a -> b

    instance InputEmployeeClass InputEmployeeData where
        getNameRaw a = name a
        getAgeRaw a = age a